=head2 sysobjectid_table()

=cut

#**********************************************************
sub sysobjectid_table1 {

  my ($attr) = @_;
  my $nms = $Nms->obj_list({ 
    IP    => '_SHOW',
    NAS_NAME => '_SHOW',
    SYS_OBJECTID => '_SHOW',
    SYS_LOCATION => '_SHOW',
    COLS_NAME => 1,
    %LIST_PARAMS,
  });


  print simple_table({ data => \@$nms, id => 'sys' });

  return 1;
}

